	<parameter name="maxStepHeight" type="float" tooltip="Maximum height for automatic walk up" group="Auto Stepping" min="0" enableAutoStepping="1">0.300000012</parameter>
	<parameter name="autoSteppingCancelAngle" type="float" tooltip="Maximum walkable slope angle.
