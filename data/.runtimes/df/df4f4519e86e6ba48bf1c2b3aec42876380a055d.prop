	<parameter name="forceMultiplier" type="float" tooltip="Force multiplier to be applied to a rigid body colliding with the player" group="Objects Interaction" min="0" enableObjectsInteraction="1">150</parameter>
	<parameter name="showAdvancedSettings" type="toggle" tooltip="Toggles advanced player settings on and off" group="Advanced Settings">0</parameter>
	<parameter name="contactsBufferSize" type="int" tooltip="Maximum number of contacts to be processed for collisions" group="Advanced Settings" min="4" max="1500" showAdvancedSettings="1">50</parameter>
	<parameter name="playerActorFps" type="int" tooltip="Minimum update rate for the player (in number of frames per second).
