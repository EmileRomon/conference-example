	<parameter name="acceleration" type="float" tooltip="Horizontal acceleration of the player" group="Common" min="0">25</parameter>
	<parameter name="damping" type="float" tooltip="Horizontal damping of the player&apos;s speed" group="Common" min="0">25</parameter>
	<parameter name="jumpPower" type="float" tooltip="Jumping power of the player" group="Common" min="0">6</parameter>
	<parameter name="gravityMultiplier" type="float" tooltip="Gravity multiplier for the player" group="Common" min="0">1.5</parameter>
	<parameter name="cameraHeight" type="float" tooltip="Camera height above the playerâ€™s feet" group="Camera" min="0">1.64999998</parameter>
	<parameter name="minThetaAngle" type="float" tooltip="The minimum vertical angle of the camera (look down)" group="Camera" min="-89.9" max="89.9">-89.9000015</parameter>
	<parameter name="maxThetaAngle" type="float" tooltip="The maximum vertical angle of the camera (look up)" group="Camera" min="-89.9" max="89.9">89.9000015</parameter>
	<parameter name="enableAutoStepping" type="toggle" tooltip="Toggles automatic walking up and down certain obstacles (stairs, for example)" group="Auto Stepping">1</parameter>
	<parameter name="minStepHeight" type="float" tooltip="Minimum height for automatic walk up.
