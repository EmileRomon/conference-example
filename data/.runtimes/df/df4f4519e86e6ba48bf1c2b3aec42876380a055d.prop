	<parameter name="collisionHeight" type="float" tooltip="Height of the capsule shape (cylindrical part only)" group="Common" min="0" useExternalBody="0">1.14999998</parameter>
	<parameter name="friction" type="float" tooltip="Static friction of the player" group="Common" min="0">2</parameter>
	<parameter name="walkSpeed" type="float" tooltip="Walking speed of the player" group="Common" min="0">4</parameter>
